    "keywords": [ "command", "command line", "cli", "getopt", "options" ],
    "homepage": "http://auraphp.github.com/Aura.Cli",
    "version": "1.0.0-dev",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura Contributors",
            "homepage": "https://github.com/auraphp/Aura.Cli/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "aura/installer-default": "1.0.0"
    },
    "autoload": {
        "psr-0": {
            "Aura\\Cli": "src/"
        }
    }
}